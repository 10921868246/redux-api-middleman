{
  "name": "redux-api-middleman",
  "version": "2.0.1",
  "description": "A Redux middleware making sending request a breeze",
  "main": "lib/index.js",
  "jest": {
    "testEnvironment": "node"
  },
  "size-limit": [
    {
      "name": "minified",
      "path": "src/index.js",
      "gzip": false
    },
    {
      "name": "gzipped",
      "path": "src/index.js"
    }
  ],
  "scripts": {
    "test": "jest",
    "test:ci": "yarn lint && jest",
    "lint": "standard --fix --verbose | snazzy",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "build": "babel src --out-dir lib",
    "clear": "rm -rf lib/*",
    "release:patch": "yarn run build && release patch",
    "release:minor": "yarn run build && release minor",
    "release:major": "yarn run build && release major",
    "prepublish": "yarn run test:ci && yarn run clear && yarn run build"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "author": {
    "name": "YangHsing Lin",
    "email": "yanghsing.lin@gmail.com"
  },
  "homepage": "https://github.com/CodementorIO/redux-api-middleman",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "jest": "^23.5.0",
    "nock": "^9.6.1",
    "release": "^3.1.6",
    "size-limit": "^0.21.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "humps": "^1.1.0",
    "lodash": "^4.13.1",
    "qs": "^6.5.2",
    "yaku": "^0.18.6"
  },
  "licence": "MIT"
}
