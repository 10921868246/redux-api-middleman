{
  "name": "redux-api-middleman",
  "version": "1.6.0",
  "description": "A Redux middleware making sending request a breeze",
  "main": "lib/index.js",
  "jest": {
    "testEnvironment": "node"
  },
  "size-limit": [
    {
      "path": "src/index.js"
    }
  ],
  "scripts": {
    "test": "jest",
    "test:ci": "jest",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "build": "babel src --out-dir lib",
    "clear": "rm -rf lib/*",
    "release:patch": "yarn run build && release patch",
    "release:minor": "yarn run build && release minor",
    "release:major": "yarn run build && release major",
    "prepublish": "yarn run test:ci && yarn run clear && yarn run build"
  },
  "author": {
    "name": "YangHsing Lin",
    "email": "yanghsing.lin@gmail.com"
  },
  "homepage": "https://github.com/CodementorIO/redux-api-middleman",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "nock": "^9.0.22",
    "release": "^3.1.6",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "size-limit": "^0.18.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.4.0",
    "humps": "^1.1.0",
    "jest": "^21.2.1",
    "lodash": "^4.13.1"
  },
  "licence": "MIT"
}
